  "devDependencies": {
    "@stencil/dev-server": "0.0.17",
    "@stencil/utils": "0.0.4",
    "api-ai-javascript": "2.0.0-beta.21"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fdom92/stencil-bot.git"
